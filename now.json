{
  "version": 2,
  "alias": ["3-stars", "3stars", "three-stars", "threestars"],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "build": {
    "env": {
      "REACT_APP_ALGOLIA_APPLICATION_ID": "@3-stars-algolia-application-id",
      "REACT_APP_ALGOLIA_API_KEY": "@3-stars-algolia-api-key"
    }
  }
}
